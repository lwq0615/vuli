{
  "name": "vuli",
  "version": "1.2.4",
  "description": "Vuli组件库",
  "main": "dist/lib/index.js",
  "keywords": [
    "vuli",
    "vue",
    "ui"
  ],
  "author": "liweiqiang",
  "files": [
    "dist",
    "packages"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:js": "webpack --config ./webpack.config.js",
    "build:css": "npx gulp css",
    "publish": "npm run build:js && npm run build:css && npm publish",
    "docs": "vuepress dev docs",
    "git": "npm run git:page && npm run git:push",
    "git:push": "git init && git add . && git commit -m 'update' && git push origin master",
    "git:page": "vuepress build docs && cd docs/.vuepress/dist && git init && git add -A && git commit -m 'deploy' && git push -f https://github.com/lwq0615/vuli.git master:gh-pages"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "axios": "^0.26.0",
    "babel-eslint": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-base64": "^0.1.3",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "node-sass": "^7.0.1",
    "sass": "^1.48.0",
    "sass-loader": "^5.0.1",
    "vuepress": "^1.9.5",
    "webpack-cli": "^4.9.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
